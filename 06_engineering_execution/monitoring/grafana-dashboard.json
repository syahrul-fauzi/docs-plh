{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": true,
  "panels": [
    {
      "title": "AI Compliance Rate (%)",
      "type": "gauge",
      "datasource": "${datasource}",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(legal_rule_violations_total) / sum(legal_rule_executions_total) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 25 }
            ]
          }
        }
      }
    },
    {
      "title": "Average Response Time (ms)",
      "type": "timeseries",
      "datasource": "${datasource}",
      "gridPos": { "h": 8, "w": 18, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "avg(legal_rule_duration_average_ms)",
          "refId": "A",
          "legendFormat": "Avg Latency"
        }
      ]
    },
    {
      "title": "Rule Execution Volume",
      "type": "timeseries",
      "datasource": "${datasource}",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "rate(legal_rule_executions_total[1m])",
          "refId": "A",
          "legendFormat": "Requests/sec"
        }
      ]
    },
    {
      "title": "Policy Violations by Rule",
      "type": "barchart",
      "datasource": "${datasource}",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum by (rule_id) (legal_rule_violations_total)",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "5s",
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["lawyers-hub", "ai", "compliance"],
  "templating": {
    "list": [
      {
        "current": {
          "selected": true,
          "text": "Prometheus",
          "value": "prometheus"
        },
        "hide": 0,
        "includeAll": false,
        "label": "Datasource",
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
